/* THEME BREWDOG */
.header-mobile__menu {
    z-index: 9999999
}

body .header-mobile {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 999999
}

/* body .header {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 999999;
    box-sizing: content-box
} */

body .header-mobile__menu {
    position: fixed;
    overflow-y: scroll;
    height: calc(100% - 80px)
}

.header-mobile__menu {
    display: none
}

.header-mobile__menu.active {
    display: block
}

.header-mobile__menu__item+div,
.header-mobile__menu__subitem__item~.header-mobile__menu__subitem__subitem {
    display: none
}

.header-mobile__menu__item.active+div {
    display: block
}

.header-mobile__menu__subitem__item.active~.header-mobile__menu__subitem__subitem {
    display: block
}

.header-mobile__menu__item .icon-open {
    display: none
}

.header-mobile__menu__item .icon-closed {
    display: block
}

.header-mobile__menu__item.active .icon-open {
    display: block
}

.header-mobile__menu__item.active .icon-closed {
    display: none
}

.header-mobile__menu__subitem__item .icon-open {
    display: none
}

.header-mobile__menu__subitem__item .icon-closed {
    display: block
}

.header-mobile__menu__subitem__item.active .icon-open {
    display: block
}

.header-mobile__menu__subitem__item.active .icon-closed {
    display: none
}

.showcart {
    display: inline-flex
}

.nav-toggle:before {
    content: ''
}

.header .header__links__link .header__menu {
    opacity: 0;
    visibility: hidden;
    z-index: 100000;
}

.header .header__links__link:hover .header__menu, .header .header__links__link:focus .header__menu {
    opacity: 1;
    visibility: visible
}

.header a {
    color: #000;
    &:hover {
	    text-decoration: underline;
    }
}

.dropdown .dropdown__menu {
    display: none
}

.dropdown.active .dropdown__menu {
    display: block
}

button,
.quote-details-items .actions-toolbar>.action {
    padding: inherit
}


