@font-face {
    font-family: New-Grotesk-Square;
    font-weight: 400;
    src: url("NewGroteskSquareFour.otf") format("opentype");
}
@font-face {
    font-family: New-Grotesk-Square;
    font-weight: 500;
    src: url("NewGroteskSquareFive.otf") format("opentype");
}
@font-face {
    font-family: New-Grotesk;
    src: url("NHaasGroteskTXStd.otf") format("opentype");
}
@font-face {
    font-family: New-Grotesk-Med;
    font-family: 400;
    src: url("NHaasGroteskTXStd-400.otf") format("opentype");
}
@font-face {
    font-family: New-Grotesk-Bld;
    font-family: 700;
    src: url("NHaasGroteskTXStd-700.otf") format("opentype");
}

input, textarea, button, select {
    font-family: 'New-Grotesk', sans-serif;
}
body {
    font-family: 'New-Grotesk-Med', sans-serif;
}

ul.header-nav-left {
	height: 70px !important;
}

h1, h2, h3, h4, h5, h6 {
    line-height: 1;
}

h1 {
	font-family: 'New-Grotesk-Square' !important;
	letter-spacing: inherit !important;
    font-weight: 400 !important;
	font-size: 64px;	
}
h2 {
    font-family: 'New-Grotesk-Square' !important;
    letter-spacing: inherit !important;
    font-weight: 400 !important;
    font-size: 64px;
    .banner-text {
		font-size: 18px;	
	}
}

h3, .h3, span.product_image_caption {
	font-family: neue-haas-grotesk-display, sans-serif !important;
	font-weight: 600;
	font-style: normal;
}
span.h3 {
	font-size: 18px;
}
span.product_image_caption, h6 {
	font-size: 24px;
}

.header-nav-left li a.topParent {
	font-family: neue-haas-grotesk-display, sans-serif !important;
	font-weight: 600;
	font-style: normal;
    text-transform: none;
	font-size: 18px !important;
    letter-spacing: inherit !important;	
}
.banner-container {
	background-color: #000 !important;
}
#header .grid__item.black {
	background: #fff;
}
#header .grid__item.black .nav a {
	color: #000 !important;
}
input, textarea, button, select {
    font-family: neue-haas-grotesk-display, sans-serif !important;
    font-weight: 600 !important;
    font-style: normal;
}
.banner-btn-text {
    background: transparent !important;
    border: none !important;
    text-decoration: underline;
    font-family: neue-haas-grotesk-display, sans-serif !important;
    font-weight: 600 !important;
    font-style: normal;
    text-transform: none !important;
}
.cart__row {
    font-family: neue-haas-grotesk-display, sans-serif;
    font-weight: 600;
    font-size: 18px;
}

.js-qty__adjust--minus, .ajaxcart__qty--minus {
    border-right: 2px solid #000;
}
.js-qty__adjust--plus, .ajaxcart__qty--plus {
    border-left: 2px solid #000;
}
.js-qty__adjust, .ajaxcart__qty-adjust {
	padding: 0 10px;
}

@media screen and (min-width: 841px) {
	.carousel.has-parallax .carousel-slide-image {
	    height: 740px;
	    background-size: cover;
      	background-position: center center;
	}		
}

.grid--featured-products img {
	background: white !important;
}

.sub {
	background: #EEECEF;
	&:after {
	    position: absolute;
	    z-index: 0;
	    top: 1rem;
	    right: 3rem;
	    opacity: 0.1;
	}
	a {
	    color: #000 !important;
	    text-transform: capitalize;
	    font-family: neue-haas-grotesk-display, sans-serif;
	    font-weight: 600;
	    font-size: 18px;
	    line-height: 1.75;
	    text-transform: capitalize !important;
	}
}

#header .grid__item.black .nav a {
	text-transform: capitalize;
	line-height: 1;
	margin-bottom: 8px;
	font-size: 18px;
}

.nav img {
    padding: 0 2px 6px 10px;
}

header .nav .sub:after {
	right: 15px;
	width: 200px;
}
.header-nav-left li:hover a.topParent {
